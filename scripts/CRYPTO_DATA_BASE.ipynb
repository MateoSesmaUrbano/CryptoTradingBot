        "from datetime import date, datetime, timedelta\n",
        "\n",
        "def perdelta(start, end, delta):\n",
        "    curr = start\n",
        "    dates = []\n",
        "    while curr < end:\n",
        "        dates.append(curr)\n",
        "        curr += delta\n",
        "    dates.append(end)\n",
        "    return dates\n",
        "\n",
        "for result in perdelta(dateparser.parse(\"Jul 15, 2021\"), dateparser.parse(\"Jul 20, 2021\"), timedelta(days=60)):\n",
        "    print(result)"
      ],
